{
  "packageManager": "yarn@4.7.0",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --experimental-specifier-resolution=node ./dist/server.js",
    "dev": "NODE_ENV=dev nodemon",
    "test": "jest --detectOpenHandles --forceExit",
    "create-test-data": "node --loader ts-node/esm src/tests/createTestData.ts",
    "lint-api-specs": "redocly lint openapi/dist/openapi.yaml",
    "generate-api-specs": "redocly bundle openapi/index.yaml --output openapi/dist/openapi.yaml"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.3",
    "pg-hstore": "^2.3.4",
    "resend": "^4.1.2",
    "sequelize": "^6.37.6"
  },
  "devDependencies": {
    "@redocly/cli": "^1.34.0",
    "@types/cookie-parser": "^1",
    "@types/cors": "^2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9",
    "@types/node": "^22.13.9",
    "@types/pg": "^8.11.11",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
